<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Settings</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.displayDuplicateImages>
                        <TCEforms>
                            <label>Display multiple used images more than once?</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.displayDuplicateImages>

                    <settings.rootlinepage>
                        <TCEforms>
                            <label>From what pages on should references be searched?</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                            </config>
                        </TCEforms>
                    </settings.rootlinepage>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>